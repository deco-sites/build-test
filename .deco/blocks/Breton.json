{
  "__resolveType": "site/sections/Theme/Theme.tsx",
  "colorScheme": "any",
  "buttonStyle": {
    "--border-btn": "1px",
    "--btn-focus-scale": "1",
    "--animation-btn": "0.25s",
    "--rounded-btn": "0"
  },
  "otherStyles": {
    "--rounded-box": "1rem",
    "--rounded-badge": "1.9rem",
    "--animation-input": "0.2s",
    "--tab-border": "1px",
    "--tab-radius": "0.5rem"
  },
  "font": {
    "__resolveType": "website/loaders/fonts/googleFonts.ts",
    "fonts": [
      {
        "variations": [
          {
            "weight": "200"
          },
          {
            "weight": "300"
          },
          {
            "weight": "400"
          },
          {
            "weight": "500"
          },
          {
            "weight": "600"
          },
          {
            "weight": "700"
          }
        ],
        "family": "Inter"
      }
    ]
